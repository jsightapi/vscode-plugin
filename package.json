{
    "name": "jschema",
    "displayName": "JSchema",
    "description": "JSight Schema Language",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.61.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "jschema",
            "aliases": ["JSchema", "jschema"],
